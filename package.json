{
  "name": "solidjs-template",
  "version": "1.0.0",
  "description": "Template for building apps with SolidJS",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "jest",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx src/",
    "lint:ci": "eslint --ext .ts,.tsx,.js,.jsx src/ --max-warnings 0",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,html,css,json}\"",
    "format:ci": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,html,css,json}\"",
    "prepare": "husky install"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-solid": "^0.1.2",
    "eslint-plugin-testing-library": "^4.12.4",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "solid-jest": "^0.2.0",
    "solid-testing-library": "^0.2.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.2",
    "vite-plugin-solid": "^2.1.0"
  },
  "dependencies": {
    "solid-js": "^1.1.5"
  }
}
